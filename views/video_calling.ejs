<html>
  <head>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css" />
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <link rel="stylesheet" href="./css/style.css" />
  </head>

  <body>
    <header>
      <nav>
        <div class="nav-wrapper">
          <a href="index.html" class="brand-logo">Logo</a>
          <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
          <ul class="right hide-on-med-and-down">
            <li>
              <form>
                <div class="input-field">
                  <input id="search" type="search" required />
                  <label class="label-icon" for="search"><i class="material-icons">search</i></label>
                  <i class="material-icons">close</i>
                </div>
              </form>
            </li>
            <li><a href="patient.html">Doctor List</a></li>
            <li><a href="p_appointment.html">Appointments</a></li>
            <li><a href="video_calling.html">Call doctor</a></li>
            <li><a href="index.html">Sign out</a></li>
          </ul>
        </div>
      </nav>

      <ul class="sidenav" id="mobile-demo">
        <li><a href="patient.html">Doctor List</a></li>
        <li><a href="p_appointment.html">Appointments</a></li>
        <li><a href="video_calling.html">Call doctor</a></li>
        <li><a href="index.html">Sign out</a></li>
      </ul>
    </header>

    <main>
      <div class="container">
        <div class="row">
          <div class="center-align">
            <h4>Communication happening now</h4>
          </div>
          <br /><br />
          <div class="col s12 l8">
            <div class="card card-rounded">
              <div class="card-content">
                <img src="./img/doctor.jpg" alt="doctor" class="responsive-img" />
                <div class="center">
                  <a class="btn-floating btn-large waves-effect waves-light amber mr1"
                    ><i class="material-icons">mic</i></a
                  >
                  <a class="btn-floating btn-large waves-effect waves-light red mr1"
                    ><i class="material-icons">phone</i></a
                  >
                  <a class="btn-floating btn-large waves-effect waves-light blue-grey darken-2 mr1"
                    ><i class="material-icons">videocam_off</i></a
                  >
                </div>
              </div>
            </div>
          </div>
          <div class="col s12 l4">
            <div class="card card-rounded">
              <div class="card-content">
                <h5 class="center" style="margin-top: 0">Prescription</h5>
                <form class="col s12">
                  <div class="row">
                    <div class="input-field col s12">
                      <input id="first_name" type="text" class="validate" />
                      <label for="first_name">Patient Name</label>
                    </div>
                    <div class="input-field col s12">
                      <input id="age" type="number" class="validate" />
                      <label for="age">Age</label>
                    </div>
                    <div class="col s12">
                      <p>
                        <label>
                          <input class="with-gap" name="group1" type="radio" checked />
                          <span>Male</span>
                        </label>
                      </p>
                      <p>
                        <label>
                          <input class="with-gap" name="group1" type="radio" />
                          <span>Female</span>
                        </label>
                      </p>
                    </div>
                    <div class="input-field col s12">
                      <input id="symptoms" type="text" class="validate" />
                      <label for="symptoms">Symptoms</label>
                    </div>
                    <div class="input-field col s12">
                      <textarea id="textarea1" class="materialize-textarea"></textarea>
                      <label for="textarea1">Medicine</label>
                    </div>
                  </div>
                  <div class="col s12">
                    <a class="waves-effect waves-light btn btn-rounded flat_btn prescription">Create prescription</a>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>

    <footer></footer>

    <script
      src="https://code.jquery.com/jquery-3.5.1.min.js"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="./js/app.js"></script>
  </body>
</html>
